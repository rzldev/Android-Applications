<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="16dp"
    android:layout_marginTop="16dp"
    android:layout_marginBottom="16dp"
    android:background="@color/colorPutih"
    android:screenOrientation="portrait">

    <ImageView
        android:id="@+id/bgjmltiket"
        android:layout_width="40dp"
        android:layout_height="58dp"
        android:background="#ffd782"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/ittJumlahTiket"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="4dp"
        android:fontFamily="sans-serif"
        android:gravity="center_horizontal"
        android:letterSpacing="0.01"
        android:lineSpacingExtra="3sp"
        android:text="10"
        android:textColor="#de000000"
        android:textSize="16sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/bgjmltiket"
        app:layout_constraintStart_toStartOf="@+id/bgjmltiket"
        app:layout_constraintTop_toTopOf="@+id/bgjmltiket" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif"
        android:gravity="center_horizontal"
        android:letterSpacing="0.01"
        android:lineSpacingExtra="2sp"
        android:text="Tiket"
        android:textColor="#de000000"
        android:textSize="12sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="@+id/bgjmltiket"
        app:layout_constraintEnd_toEndOf="@+id/bgjmltiket"
        app:layout_constraintStart_toStartOf="@+id/bgjmltiket"
        app:layout_constraintTop_toBottomOf="@+id/ittJumlahTiket" />

    <TextView
        android:id="@+id/ittNamaWisata"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="sans-serif"
        android:letterSpacing="0.01"
        android:lineSpacingExtra="3sp"
        android:text="Gunung Bromo"
        android:textColor="#de000000"
        android:textSize="16sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/bgjmltiket"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/ittWilayahWisata"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="2dp"
        android:layout_marginBottom="16dp"
        android:fontFamily="sans-serif-medium"
        android:text="Surabaya"
        android:textColor="@color/colorButton"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@id/bgjmltiket"
        app:layout_constraintStart_toEndOf="@id/bgjmltiket"
        app:layout_constraintTop_toBottomOf="@id/ittNamaWisata" />

    <TextView
        android:id="@+id/ittTanggalKunjungan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:fontFamily="sans-serif"
        android:letterSpacing="0.03"
        android:lineSpacingExtra="2sp"
        android:text="Minggu, 28 April 2019"
        android:textColor="#99000000"
        android:textSize="12sp"
        android:textStyle="normal"
        app:layout_constraintStart_toEndOf="@id/bgjmltiket"
        app:layout_constraintTop_toBottomOf="@+id/ittWilayahWisata" />

    <TextView
        android:id="@+id/ittTotalHarga"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:text="Rp 99000"
        android:textColor="#000"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:background="#e3e3e3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/bgjmltiket"
        app:layout_constraintTop_toBottomOf="@id/bgjmltiket" />

